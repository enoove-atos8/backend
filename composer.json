{
    "name": "enoove/atos8",
    "type": "project",
    "description": "Manage church platform",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "darkaonline/l5-swagger": "^8.6",
        "fedeisas/laravel-mail-css-inliner": "^5.3",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^11.9",
        "laravel/octane": "^v2.5.2",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.28",
        "mailersend/laravel-driver": "^2.6",
        "sentry/sentry-laravel": "^4.18",
        "spatie/browsershot": "^5.0",
        "spatie/data-transfer-object": "^3.9",
        "spatie/laravel-permission": "*",
        "spatie/pdf-to-text": "^1.54",
        "stancl/tenancy": "^3.8",
        "symfony/css-selector": "^7.1",
        "symfony/http-client": "^7.3",
        "symfony/mailgun-mailer": "^7.3",
        "thiagoalessio/tesseract_ocr": "^2.13",
        "zircote/swagger-php": "^4.10"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/boost": "^1.3",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Application\\": "app/Application/",
            "Domain\\": "app/Domain/",
            "Support\\": "app/Support/",
            "Infrastructure\\": "app/Infrastructure/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Seeders\\Central\\": "database/seeders/central/",
            "Database\\Seeders\\Tenant\\": "database/seeders/tenant/"
        },
        "files": [
            "app/Application/Core/Helpers/PathHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 1200,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
