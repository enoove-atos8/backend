        "/v1/church": {
            "post": {
                "tags": [
                    "Church"
                ],
                "summary": "Register a new church to access the platform",
                "operationId": "9adf9452ae836670bc867e33f6bd07a1",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "church": {
                                        "properties": {
                                            "tenant_id": {
                                                "type": "integer",
                                                "example": "ibrr"
                                            },
                                            "plan_id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "name": {
                                                "type": "string",
                                                "example": "Igreja Batista Reformada do Recife"
                                            },
                                            "activated": {
                                                "type": "boolean",
                                                "example": false
                                            },
                                            "doc_type": {
                                                "type": "string",
                                                "example": "cnpj/cpf"
                                            },
                                            "doc_number": {
                                                "type": "string",
                                                "example": "53575672000191/90441944027"
                                            }
                                        },
                                        "type": "object"
                                    },
                                    "user": {
                                        "properties": {
                                            "admin_email_tenant": {
                                                "type": "string",
                                                "example": "admin_email_tenant@ibrr.com.br"
                                            },
                                            "pass_admin_email_tenant": {
                                                "type": "string",
                                                "example": "123456"
                                            },
                                            "confirm_pass_admin_email_tenant": {
                                                "type": "string",
                                                "example": "same password"
                                            },
                                            "user_activated_tenant": {
                                                "type": "boolean",
                                                "example": false
                                            },
                                            "user_type_tenant": {
                                                "type": "string",
                                                "example": "web"
                                            }
                                        },
                                        "type": "object"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "New church registred successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "church": {
                                            "properties": {
                                                "tenant_id": {
                                                    "type": "integer",
                                                    "example": "ibrr"
                                                },
                                                "name": {
                                                    "type": "string",
                                                    "example": "Igreja Batista Reformada do Recife"
                                                },
                                                "activated": {
                                                    "type": "boolean",
                                                    "example": false
                                                },
                                                "doc_type": {
                                                    "type": "string",
                                                    "example": "cnpj/cpf"
                                                },
                                                "doc_number": {
                                                    "type": "string",
                                                    "example": "53575672000191/90441944027"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity"
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "404": {
                        "description": "Resource Not Found"
                    }
                },
                "security": [
                    {
                        "Bearer": []
                    }
                ]
            }
        },
